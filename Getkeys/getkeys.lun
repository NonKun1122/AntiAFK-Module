local API_URL = "https://gat-keys.netlify.app/verifyKey"

local function checkKey(key)
    local ok, response = pcall(function()
        return game:HttpGet(API_URL.."?key="..key)
    end)
    if not ok then return false, "‡πÑ‡∏°‡πà‡∏™‡∏≤‡∏°‡∏≤‡∏£‡∏ñ‡πÄ‡∏ä‡∏∑‡πà‡∏≠‡∏°‡∏ï‡πà‡∏≠ API" end

    local success, data = pcall(function()
        return HttpService:JSONDecode(response)
    end)
    if not success then return false, "API ‡∏ï‡∏≠‡∏ö‡∏Å‡∏•‡∏±‡∏ö‡πÑ‡∏°‡πà‡∏ñ‡∏π‡∏Å‡∏ï‡πâ‡∏≠‡∏á" end

    return data.valid, data.reason
end

